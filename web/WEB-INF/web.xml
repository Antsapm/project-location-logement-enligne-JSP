<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>com.servlet.HomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.servlet.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertionAjax</servlet-name>
        <servlet-class>com.ajax.InsertionAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeConnecterAjax</servlet-name>
        <servlet-class>com.ajax.SeConnecterAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeDeconnecterAjax</servlet-name>
        <servlet-class>com.ajax.SeDeconnecterAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfilServlet</servlet-name>
        <servlet-class>com.servlet.ProfilServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PublicationAjax</servlet-name>
        <servlet-class>com.ajax.PublicationAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificationAjax</servlet-name>
        <servlet-class>com.ajax.ModificationAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>JaimeAjax</servlet-name>
        <servlet-class>com.ajax.JaimeAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>disJaimeAjax</servlet-name>
        <servlet-class>com.ajax.disJaimeAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CommentaireAjax</servlet-name>
        <servlet-class>com.ajax.CommentaireAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewCommentaireAjax</servlet-name>
        <servlet-class>com.ajax.NewCommentaireAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>com.servlet.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/HomeServlet</url-pattern>
    </servlet-mapping>
    <jsp-config>
        <jsp-property-group>
            <url-pattern>*.jsp</url-pattern>
            <include-prelude>/WEB-INF/includes/taglibs.jsp</include-prelude>
        </jsp-property-group>
    </jsp-config>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertionAjax</servlet-name>
        <url-pattern>/InsertionAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeConnecterAjax</servlet-name>
        <url-pattern>/SeConnecterAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeDeconnecterAjax</servlet-name>
        <url-pattern>/SeDeconnecterAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfilServlet</servlet-name>
        <url-pattern>/ProfilServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PublicationAjax</servlet-name>
        <url-pattern>/PublicationAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificationAjax</servlet-name>
        <url-pattern>/ModificationAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>JaimeAjax</servlet-name>
        <url-pattern>/JaimeAjax</url-pattern>
    </servlet-mapping>
    
    <servlet-mapping>
        <servlet-name>disJaimeAjax</servlet-name>
        <url-pattern>/disJaimeAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CommentaireAjax</servlet-name>
        <url-pattern>/CommentaireAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewCommentaireAjax</servlet-name>
        <url-pattern>/NewCommentaireAjax</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/SearchServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
